#include <string>
#include <vector>
#include <iostream>

using namespace std;

vector<vector<int>> solution(vector<vector<int>> arr1, vector<vector<int>> arr2) {
    vector<vector<int>> answer;

    for(int i=0; i<arr1.size(); i++){
        vector<int> sum;
        for(int j=0; j<arr1[i].size(); j++){
            int s = (arr1[i][j] + arr2[i][j]);
            sum.push_back(s);
        }
        answer.push_back(sum);
        sum.clear();
    }

    return answer;
}
